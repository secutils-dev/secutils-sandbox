{
  "name": "@secutils-dev/kibana-security-health-check",
  "version": "1.0.0",
  "targets": {
    "fetch-api": {
      "source": "./src/fetch-api.ts",
      "distDir": "../dist/content-extractor-scripts",
      "context": "browser",
      "outputFormat": "esmodule",
      "isLibrary": true
    },
    "github-codeowner-file": {
      "source": "./src/github-codeowner-file.ts",
      "distDir": "../dist/content-extractor-scripts",
      "context": "node",
      "includeNodeModules": true,
      "optimize": true,
      "outputFormat": "esmodule"
    },
    "markdown-table": {
      "source": "./src/markdown-table.ts",
      "distDir": "../dist/content-extractor-scripts",
      "context": "node",
      "includeNodeModules": true,
      "optimize": true,
      "outputFormat": "esmodule"
    },
    "kibana-security-health-check": {
      "source": "./src/kibana-security-health-check/index.ts",
      "distDir": "../dist/content-extractor-scripts",
      "context": "browser",
      "outputFormat": "esmodule",
      "isLibrary": true
    }
  },
  "browserslist": ["last 2 Chrome versions"],
  "dependencies": {
    "markdown-table": "^3.0.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@octokit/types": "^14.1.0",
    "@parcel/config-default": "^2.15.4",
    "@parcel/packager-ts": "^2.15.4",
    "@parcel/transformer-typescript-tsc": "^2.15.4",
    "@parcel/transformer-typescript-types": "^2.15.4",
    "@parcel/validator-typescript": "^2.15.4",
    "@types/node": "^22.17.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "parcel": "^2.15.4",
    "playwright-core": "1.52.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "prebuild": "eslint ../content-extractor-scripts",
    "watch": "parcel watch",
    "build": "parcel build --no-content-hash"
  }
}
